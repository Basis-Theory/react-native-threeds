{
  "name": "@basis-theory/react-native-threeds",
  "version": "1.0.0",
  "description": "Basis Theory 3DS React Native SDK",
  "author": "Basis Theory <support@basistheory.com> (https://basistheory.com)",
  "license": "Apache-2.0",
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "example": "yarn workspace 3ds-react-native-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint --quiet",
    "clean": "del-cli dist",
    "build": "bob build && node prepare.js",
    "release": "semantic-release"
  },
  "source": "./src/index.tsx",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/typescript/module/src/index.d.ts",
        "default": "./dist/module/index.js"
      },
      "require": {
        "types": "./dist/typescript/commonjs/src/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.json",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "dependencies": {
    "react-native-get-random-values": "^1.11.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.13.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.73.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.8.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-builder-bob": "^0.30.2",
    "react-native-webview": "^13.12.3",
    "react-test-renderer": "^18.3.1",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.12.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-webview": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "library",
    "version": "0.42.0"
  }
}
